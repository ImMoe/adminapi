
@{
    ViewBag.Title = "Login";
}

<h3 class="logo">Logga in</h3>
<div class="login">
    <form action="" method="post" autocomplete="off">
        <div class="group">
            <label for="username">Användarnamn</label>
            <input type="text" placeholder="Admin användarnamn" name="username" />
        </div>
        <div class="group">
            <label for="password">Lösenord</label>
            <input type="password" placeholder="Admin lösenord" name="password" />
        </div>
        <button class="button">Logga in</button>
    </form>
</div>

